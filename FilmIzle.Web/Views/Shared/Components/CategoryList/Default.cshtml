@model List<CategoryListDto>



<div style="border:dashed;">
    <h3 class="text-white m-2">İzlemek istediğiniz film türünü seçiniz</h3>
    @foreach (var item in Model)
    {
        <a class="btn btn-outline-light my-2 mx-3" asp-action="Category" asp-route-name="@item.Name">@item.Name</a>
    }
</div>